/** 根据省市名称获取店铺编码**/ 
function getShopIdByTheName(){

    var shopId = "10000";//默认店铺ID集团
    var theName = $("#shengfen").text();
    if(theName == "集团"){
        shopId = "10000";
    }else if(theName == "北京"){
        shopId = "10001";
    }else if(theName == "天津"){
        shopId = "10002";
    }else if(theName == "辽宁"){
        shopId = "10005";
    }else if(theName == "河北"){
        shopId = "10006";
    }else if(theName == "山西"){
        shopId = "10007";
    }else if(theName == "内蒙古"){
        shopId = "10008";
    }else if(theName == "宁夏"){
        shopId = "10009";
    }else if(theName == "黑龙江"){
        shopId = "10010";
    }else{
        shopId = getCookieProve('SHOPID_COOKIEID');
    }
    
    return shopId;
}




function goToLiangHao(){
    var shopCode = getShopIdByTheName();
    window.location.href ='/dqmh/tianyiMall/goodNumAction.do?method=getProvAreaForSelect&shopCode='+shopCode+'&pageSize=24&t='+new Date().getTime();
}

function goToLHHYJ(){
    var shopCode = getShopIdByTheName();
    window.location.href ='/dqmh/tianyiMall/goodNumAction.do?method=getProvAreaForSelectLHHYJ&shopCode='+shopCode+'&pageSize=24&t='+new Date().getTime();
}
function hidermtj(){
    var ifram = document.getElementsByTagName("iframe");
    for(var am in ifram){
        if(ifram[am].src != null && ifram[am].src.indexOf("/mall/rmtj")>0){
            ifram[am].id="rmtjId";
            $("#rmtjId").hide();
            break;
        }
    } 
}
</script>
