<script type="text/javascript">
	$(function(){	
            $(".me").click(function(){
			    if($(this).hasClass("hur")){
			         $(this).removeClass("hur");
			    }else{
                     $(this).addClass("hur");}
             });
     });
</script>
<!--tab切换-->
<script type="text/javascript">
	$(function(){	
            $(".dt").click(function(){
			    if($(this).hasClass("dtl")){
			    }else{
                     $(this).addClass("dtl");
					 $(".lb").removeClass('lbl');
					 $(".lbzs").show();
					 $(".dtzs").hide();
					 }
             });
			 $(".lb").click(function(){
			    if($(this).hasClass("lbl")){
			    }else{
                     $(this).addClass("lbl");
					 $(".dt").removeClass('dtl');
					 $(".dtzs").show();
					 $(".lbzs").hide();
					 }
             });
     });
</script>
<!--更多、收起按钮切换-->
<script type="text/javascript">
//更多按钮根据行数展示
function moreShow(){
	var clearfixObj=$(".itemShow .clearfix");
	clearfixObj.each(function(idex,item){
		var initHeight=40;
		var compareObjCss=$(item).parent().parent().css("display");
		//alert("compareObjCss=="+compareObjCss);
		if(compareObjCss!="none"){
			var currentheight=$(item).height(); 
			currentheight=parseInt(currentheight);
			if(currentheight>initHeight){
				$(item).find(".more").css("display","block");
			}else{
				$(item).find(".more").css("display","none");
			}
		}
		
	});
}
$(function (){
	moreShow();
	$(".qian .more").unbind("click");
	$(".qian .more").click(function(){
		$(this).closest(".qian").css("height","auto");
		$(this).hide();
		$(this).next(".moresq").show();
	});
	$(".qian .moresq").unbind("click");
	$(".qian .moresq").click(function(){
		$(this).closest(".qian").css("height","40px");
		$(this).hide();
		$(this).prev(".more").show();
	});
});

</script>

<!--点选小图-->
<script type="text/javascript">
$(function(){
 	$(".picId ul div").unbind("click");
 	$(".picId ul div").click(function(){ 
 		$(this).removeClass("pictureCs1").addClass("pictureCs2");
		$(this).children().css("border","1px solid red");
		$(this).siblings().removeClass("pictureCs2").addClass("pictureCs1");
		$(this).siblings().children().css("border","1px solid #e5e5e5");
		var src=$(this).children().attr("src");
		var code=$(this).children().attr("code");
		var longTitle=$(this).children().attr("longTitle")=="null"?"":$(this).children().attr("longTitle");
		var secondTitle=$(this).children().attr("secondTitle")=="null"?"":$(this).children().attr("secondTitle");
		var cuxiaoyu=$(this).parent().parent().siblings(".wz").find(".cuxiaoyu").text();
		
		var href="http://www.189.cn/products/"+code+".html";
		$(this).parent().parent().siblings().not($(".ax")).children().attr("src",src);
		$(this).parent().parent().siblings().not($(".ax")).eq(4).attr("href",href);
		$(this).parent().parent().siblings().not($(".ax")).eq(4).html(longTitle+'<span style="font-size:12px;font-family:宋体">'+secondTitle+'</span><span class="cuxiaoyu">'+cuxiaoyu+'</span>');
		$(this).parent().parent().siblings().not($(".ax")).attr("href",href);
 	});
 	$(".sousuolz div div").unbind("click");
 	$(".sousuolz div div").click(function(){
 		$(this).removeClass("pictureCs1").addClass("pictureCs2");
		$(this).children().css("border","1px solid red");
		$(this).siblings().removeClass("pictureCs2").addClass("pictureCs1");
		$(this).siblings().children().css("border","1px solid #e5e5e5");
		var src=$(this).children().attr("src");
		var code=$(this).children().attr("code");
		var longTitle=$(this).children().attr("longTitle")=="null"?"":$(this).children().attr("longTitle");
		var secondTitle=$(this).children().attr("secondTitle")=="null"?"":$(this).children().attr("secondTitle");
		var cuxiaoyu=$(this).parent().parent().parent().parent().siblings().find("p a .cuxiaoyu").text();
		
		var href="http://www.189.cn/products/"+code+".html";
		$(this).parent().parent().parent().parent().children().attr("src",src);
		$(this).parent().parent().siblings().not($(".ax")).eq(4).attr("href",href);
		$(this).parent().parent().parent().parent().siblings().find("p a").html(longTitle+'<span style="font-size:14px;font-family:微软雅黑">'+secondTitle+'</span><span class="cuxiaoyu">'+cuxiaoyu+'</span>');
		$(this).parent().parent().siblings().not($(".ax")).attr("href",href);
 	});
});


</script>
