<!-- 收藏关注功能 -->
<script type="text/javascript">
function attention(that,index){
	var productId ="";
	var shop_id="";
	if(index==1){
		productId=$(that).closest(".ax").siblings(".fenxiangurl").attr("salesProdId");
		shop_id=$(that).closest(".ax").siblings(".fenxiangurl").attr("salesShopId");
	}else if(index==2){
		productId=$(that).closest(".sslby").siblings(".sslbz").find(".fenxiangurl").attr("salesProdId");
		shop_id=$(that).closest(".sslby").siblings(".sslbz").find(".fenxiangurl").attr("salesShopId");
	}
	 var url="/dqmh/tradingCenter/myCollection.do?method=isUser";
	 var params = {}
	 $.post(url,params,function(data){
		var json = eval(data);
		if(json=="0"){
			var options = {
				autoOpen: false,
				title:"我的关注",
				draggable :  true,
				bgiframe: true,  
				width: '400',
				height:'265'
			};
			var dlg = fin_dialog(0,"我的关注",
				"/dqmh/tradingCenter/myCollection.do?method=addMyCollection&productId="+productId+"&shop_id="+shop_id,options);
		}else{
			var locate = window.location.href;
			window.location.href="/dqmh/tradingCenter/myCollection.do?method=loginGoHistory&returnUrl="+locate;
		}
	 });
}
//关闭关注窗口
function submit_notice() {
	$("#0_dialog").dialog('close');
}
</script>
