<!-- 分享功能 -->
<script type="text/javascript">
//分享的url
var fenxiangurl=''; 
if(document.getElementById('fenxiangurl')) {
    fenxiangurl = document.getElementById('fenxiangurl').href;
} 
//自定义分享的信息
var jiathis_config = { 
pic:"",
url: fenxiangurl, 
title: "", 
summary:""
}; 					
//列表分享功能展示						
function view(that){
	jiathis_config.url=$(that).closest(".ax").siblings(".fenxiangurl").attr("href");
	$(that).mouseenter(
		function(){$(that).siblings(".jiathis_style").show();loadShareJs();}
	);
	$(that).closest(".ax").mouseleave(
		function(){$(that).siblings(".jiathis_style").hide();}
	);
}
//滑动分享
function mouseover(that,index){
	$(that).siblings(".jiathis_style").show();
	//loadShareJs();
	if(index==1){
		jiathis_config.url=$(that).closest(".ax").siblings(".fenxiangurl").attr("href");
	}else if(index==2){
		jiathis_config.url=$(that).closest(".sslby").siblings(".sslbz").find(".fenxiangurl").attr("href");
	}else{
		jiathis_config.url="";
	}
	var shareA=$(that).siblings("#fenxiangview2").find("a");
	var len=shareA.length;
	shareA.each(function(idex,item){
		if(idex!=len-1){
			$(item).find("span").text("");
		}else{
			$(item).text("");
		}
	});
	$(that).closest(".ax").mouseleave(function(){
			$(that).siblings(".jiathis_style").hide();
		}
	);
}

//大图分享功能展示						
function view2(that){
	jiathis_config.url=$(that).closest(".ax").siblings(".fenxiangurl").attr("href");
	$(that).mouseenter(
		function(){$(that).siblings(".jiathis_style").show();loadShareJs();}
	);
	$(that).closest(".ax").mouseleave(
		function(){$(that).siblings(".jiathis_style").hide();}
	);
}
//加载分享功能js文件
function loadShareJs(){
	var oHead = document.getElementsByTagName('HEAD').item(0);
	var oScript= document.createElement("script");
	oScript.type = "text/javascript";
	oScript.src="http://v2.jiathis.com/code/jia.js";
	oHead.appendChild( oScript);
}
//收藏功能
/*function AddFavorite(sURL, sTitle) {
	attention();
	 try {
	 window.external.addFavorite(sURL, sTitle); 
	}catch (e) {
		try {
			window.sidebar.addPanel(sTitle, sURL, ""); 
		} catch (e){
			alert("加入收藏失败，请使用Ctrl+D进行添加"); 
		} 
	}  
} */
</script>
