<!--小图左右移动-->
<script type="text/javascript">
function leftRunBigPic(that){
	var ulObj=$(that).siblings(".picId").find("ul");
	var position = $(ulObj).css("marginLeft");  
	var img_length=$(ulObj).children().length;
	var totle_length=img_length*35-175;
	position=parseInt(position.replace("px",""));
	if(position<0){
	$(ulObj).animate({marginLeft:parseInt(position)+42+"px"},10);
	}
}
function rightRunBigPic(that){ 
	var ulObj=$(that).siblings(".picId").find("ul");
	var position = $(ulObj).css("marginLeft");  
	var img_length=$(ulObj).children().length;
	var totle_length=img_length*35-175;
	position=parseInt(position.replace("px",""));
	if(position>-totle_length){
		$(ulObj).animate({marginLeft:position-42+"px"},10);
	}
}
function leftRunLinePic(that){
	var ulObj=$(that).siblings(".picId").find("ul");
	var position = $(ulObj).css("marginLeft");  
	var img_length=$(ulObj).children().length;
	var totle_length=img_length*35-105;
	position=parseInt(position.replace("px",""));
	if(position<0){
	$(ulObj).animate({marginLeft:parseInt(position)+42+"px"},10);
	}
}

function rightRunLinePic(that){
	var ulObj=$(that).siblings(".picId").find("ul");
	var position = $(ulObj).css("marginLeft");  
	var img_length=$(ulObj).children().length;
	var totle_length=img_length*35-105;
	position=parseInt(position.replace("px",""));
	if(position>-totle_length){
		$(ulObj).animate({marginLeft:position-42+"px"},10);
	}
} 
</script>
<!--展开全部-->
<script type="text/javascript">
	$(function(){
		var itemLength=3;//过滤属性收起时默认显示条数
		var identify=false;
		$("#shuxingDiv .qian").each(function(idex,item){
			  var display=$(item).css("display");
			  if(display!="none"){
				  $(item).addClass("itemShow").css("height","40px");
			  }
		   });
		var moreText="";
		var len=$(".zj .itemShow").length;
		$("#shuxingDiv .itemShow").each(function(idex,item){
			   if(idex>itemLength-1){
				   identify=true;
				   $(this).hide();
				   if(idex+1!=len){
					   moreText+=$(this).find(".clearfix .mz").text().substring(0,$(this).find(".clearfix .mz").text().indexOf("："))+",";
				   }else{
					   moreText+=$(this).find(".clearfix .mz").text().substring(0,$(this).find(".clearfix .mz").text().indexOf("："));
				   }
			   }
			  
		   });
		 if(identify){
			 if(moreText!=""){
				 $(".zhankai .zkmore a").html("展开更多（"+moreText+"）");
			 }else{
				 $(".zhankai .zkmore a").html("展开更多");
			 }
	         $(".zhankai .zkmore a").click(
			  function(){
				   $(".zkmore").parent().css('display','none');
				   $(".shouqi").parent().css('display','block');
				   $("#shuxingDiv .itemShow").each(function(idex,item){
						   $(this).show();
				   });
	          });
			  $(".zhankai .shouqi a").click(
			  function(){
				   $(".zkmore").parent().css('display','block');
				   $(".shouqi").parent().css('display','none');
				   $("#shuxingDiv .itemShow").each(function(idex,item){
					   if(idex>itemLength-1){
						   $(this).hide();
					   }
				   });
	          });
		 }else{
			 $(".zhankai").css('display','none');
		 }
     });
</script>
<!--点击多选-->
<script type="text/javascript">
	$(function(){	
         var liobj=$(".zj .bq");
         liobj.each(function(){
            $(".moredx").click(function(){
               $(this).parent().parent().removeClass("qian");
       		   $(this).parent().parent().attr("style","");
       		   $(this).siblings(".more").hide();
       		   $(this).siblings(".moresq").hide();
               $(this).parent().parent().addClass("hou");
       		   $(".hou li a").unbind("click");
       		   $(this).siblings("ul").find("li a").removeClass("hurry");
       		   //$(this).parent().siblings(".qdqx").find(".confirm").attr("disabled","disabled");
             });
			 $(".qx").click(function(){
			   $(this).parent().siblings(".clearfix").find("ul li a").removeClass("hurry");
			   $(this).siblings(".confirm").removeAttr("disabled");
		  	   $(this).parent().parent().removeClass("hou");
	           $(this).parent().parent().addClass("qian");
	           $(this).closest(".qian").css("height","40px");
	           $(this).parent().parent().find(".more").show();
	           moreShow();
             });
         });
     });
</script>
